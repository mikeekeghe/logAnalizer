<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox id="AnchorPane" prefHeight="561.0" prefWidth="600" style="-fx-background-color: C1D9F9;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="loganalyzer.FXML_LogAnalyzerController">

   <children>
      <AnchorPane id="AnchorPane" prefHeight="560.0" prefWidth="600.0" style="-fx-background-color: ffc39a;" styleClass="mainFxmlClass" stylesheets="@darktheme.css">
         <children>
            <Label layoutX="254.0" layoutY="12.0" text="LOG ANALYZER" />
            <TextArea fx:id="txtInput" layoutX="15.0" layoutY="86.0" prefHeight="295.0" prefWidth="403.0" promptText="enter input here" />
            <TextArea fx:id="txtOutput" layoutX="15.0" layoutY="401.0" prefHeight="146.0" prefWidth="403.0" promptText="output shows here" />
            <Button fx:id="btnExtract" layoutX="430.0" layoutY="86.0" mnemonicParsing="false" onAction="#onClickedAnalyzeBtn" text="Start Analysis" />
            <Button fx:id="btnUrl" layoutX="423.0" layoutY="42.0" mnemonicParsing="false" text="Get from URL" />
            <TextField fx:id="txtUrl" layoutX="14.0" layoutY="42.0" prefHeight="32.0" prefWidth="403.0" promptText="enter input URL here" />
            <TextField fx:id="txtCount" editable="false" layoutX="442.0" layoutY="304.0" prefHeight="32.0" prefWidth="129.0" text="0" />
            <Button fx:id="btnReset" layoutX="442.0" layoutY="483.0" mnemonicParsing="false" onAction="#onClickedResetBtn" text="Reset All" />
         </children>
      </AnchorPane>
   </children>
</VBox>
